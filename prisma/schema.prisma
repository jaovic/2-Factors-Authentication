generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id             String             @id @default(auto()) @map("_id") @db.ObjectId
  cpf            String
  email          String
  name           String
  password       String
  createdAt      DateTime           @default(now())
  updatedAt      DateTime           @updatedAt
}